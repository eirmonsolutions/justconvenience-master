                    $(document).on('keyup', '.indentification_card', function(e) { 
                        var Number_val = $(this).val(); 

                        // length should be < 10  
                        if(Number_val.length > String(9)){
                            const accepted_length = Number_val.substring(0, 9);
                            $(".indentification_card").val(accepted_length);
                        }
                    });

                    $(document).on('change', '.indentification_card', function(e) { 
                        var Number_val = $(this).val(); 
                         
                        // Check First 2 Digits (should be > 1 and < 24) 
                        var empty_val = "";
                        const digitoRegion = Number_val.substring(0, 2);
                        if (digitoRegion < String(1) || digitoRegion > String(24)) {
                            console.log("digitoRegion = " + digitoRegion);
                            $("#passport_error").show();
                            $(".indentification_card").val(''); 
                            setTimeout(function () {
                                $("#passport_error").hide();
                            }, 3500);
                            return false; 
                        }
                        

                        // Check 3rd number (should be less than 6) 
                        // if (Number_val.length === 3) {
                        const third_digit = Number(Number_val.substring(2, 3));
                        if (third_digit > String(5)) {
                            // console.log("third_digit = " + third_digit);
                            $("#passport_error").show();
                            $(".indentification_card").val('');
                            setTimeout(function () {
                                $("#passport_error").hide();
                            }, 3500);
                            return false;
                        }
                        // }
                         
                        if(Number_val.length === 9){
                            var coefficient = [2,1,2,1,2,1,2,1,2]; 
                            var number_array = [];
                            for (var i=0; i < Number_val.length; i++) {
                                number_array.push(Number_val[i]);
                            }
                            
                            // multiply numbers with coffiicent
                            var multiply_array = [];
                            var total_sum = 0;
                            for(var k=0; k < Number_val.length; k++){
                                var sum = 0;
                                sum = Number_val[k] * coefficient[k];
                                if( sum >= 10 ){
                                    sum = sum -9;
                                }
                                total_sum +=  sum; 
                            } 
                             // console.log("Total Sum = " + total_sum);
                             var last_digit = 30 - total_sum;
                             // console.log('last_digit = ' + last_digit);
                             var Number_val_new = Number_val += last_digit;
                             // console.log('modified number = ' + Number_val1);
                            $(".indentification_card").val(Number_val_new);

                        }else {
                            $("#passport_error").show();
                            $(".indentification_card").val('');
                            setTimeout(function () {
                                $("#passport_error").hide();
                            }, 3500);
                            return false;
                        } 
                    });


                    // Names validation for numbers
                    $( document ).ready(function() {

                        $(document.body).on("paste","#NOMBRE_num, .last_name", function(event){ 
                            if (event.originalEvent.clipboardData.getData('Text').match(/[^\a-z\s\A-Z]/)) {
                                    event.preventDefault();
                                  }
                            
                        });

                        $(document.body).on("autocomplete","#NOMBRE_num, .last_name", function(event){ 
                            if (event.originalEvent.clipboardData.getData('Text').match(/[^\a-z\s\A-Z]/)) {
                                    event.preventDefault();
                                  }
                            
                        });

                        $(document.body).on("paste",".num_bill ", function(event){  
                            if (event.originalEvent.clipboardData.getData('Text').match(/[^\d]/)) {
                                event.preventDefault();  
                            }

                            var $this = $(this);  
                            var max = 6;  
                            setTimeout(function () {  
                                var text = $this.val(); 
                                if (text.length >= max) {  
                                    event.preventDefault(); 
                                    $this.val(text.substring(0, max)); 
                                } else {
                                    $this.removeClass("error");
                                }
                            }, 100);
                             
                        });

                        $(document.body).on("keyup",".num_bill ", function(event){  
                            var $this = $(this); //chache your element
                            var max = 6; //set max-value for input
                            setTimeout(function () { //workaround for catching the pasted input
                                var text = $this.val(); //get the value of the textarea
                                if (text.length >= max) { //check for the length
                                    event.preventDefault(); 
                                    $this.val(text.substring(0, max)); //cut the input to the max
                                } else {
                                    $this.removeClass("error");
                                }
                            }, 100);
                        });
                    });



                </script>
